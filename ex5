import java.util.Scanner;

public class Ex5 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        final int TAM = 10;
        int[] A = new int[TAM];
        int soma = 0;

        for (int i = 0; i < 10; i++) {
            A[i] = sc.nextInt();
            soma += A[i];
        }

        double media = soma / 10.0;
        System.out.println("MÃ©dia = " + media);
    }
}
