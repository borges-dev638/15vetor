public class Ex7 {
    public static void main(String[] args) {

        final int TAM = 10;
        int[] A = {TAM};
        int[] B = new int[TAM];

        for (int i = 0; i < 10; i++) {
            B[i] = A[i];
        }

        for (int i = 0; i < 10; i++) {
            System.out.print(B[i] + " ");
        }
    }
}
